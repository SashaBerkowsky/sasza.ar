{
  "framework": null,
  "buildCommand": "zola build",
  "outputDirectory": "public",
  "headers": [
    {
      "source": "/wasm/(.*)\\.wasm",
      "headers": [
        { "key": "Content-Type", "value": "application/wasm" }
      ]
    },
    {
      "source": "/wasm/(.*)\\.js",
      "headers": [
        { "key": "Content-Type", "value": "application/javascript" }
      ]
    }
  ]
}
